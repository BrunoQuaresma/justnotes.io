{
  "name": "notes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@types/jest": "24.0.15",
    "@types/js-cookie": "^2.2.2",
    "@types/node": "12.6.8",
    "@types/reach__router": "^1.2.4",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/reactstrap": "^8.0.1",
    "faunadb": "^2.7.0",
    "faunadb-migrate": "https://github.com/BrunoQuaresma/faunadb-migrate/#master",
    "husky": "^3.0.1",
    "js-cookie": "^2.2.0",
    "lint-staged": "^9.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "reactstrap": "^8.0.1",
    "timeago.js": "^4.0.0-beta.2",
    "ts-node": "^8.3.0",
    "typescript": "3.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "setup": "export FAUNADB_SECRET=$FAUNADB_ADMIN_KEY && faunadb-migrate setup && faunadb-migrate migrate",
    "format": "prettier-eslint --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "quotes": [
        "error",
        "single"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier-eslint-cli": "^5.0.0"
  }
}
